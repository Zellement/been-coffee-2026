{
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "clean:dsstore": "find . -name '.DS_Store' -depth -delete",
        "dev": "npm run clean:dsstore && nuxt dev",
        "dev-ssl": "NODE_TLS_REJECT_UNAUTHORIZED=0 npm run clean:dsstore && nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "lint": "npm-run-all --parallel lint:*",
        "lint:js": "npx eslint --no-error-on-unmatched-pattern --fix {components,layouts,pages,plugins,stores,utils,test,middleware}/**/*.{js,ts,vue}",
        "lint:css": "stylelint --config ./.stylelintrc --fix \"app/assets/**/*.css\"",
        "format": "npm-run-all --serial format:check format:write",
        "format:check": "npx prettier --check \"**/(*.vue|*.js|*.ts|*.md|*.scss)\"",
        "format:write": "npx prettier --write \"**/(*.vue|*.js|*.ts|*.md|*.scss)\"",
        "generate-sb-types": "storyblok generate-typescript-typedefs --sourceFilePaths storyblok/components.XXXXXXX.json --destinationFilePath types/storyblok-component-types.d.ts",
        "pull-sb-components": "storyblok pull-components --path storyblok/ --space XXXXXXX",
        "update-storyblok": "npm run pull-sb-components && npm run generate-sb-types"
    },
    "devDependencies": {
        "@nuxt/eslint": "^1.12.1",
        "@nuxt/scripts": "^0.13.0",
        "@nuxtjs/seo": "^3.3.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-nuxt": "4.0.0",
        "eslint-plugin-vue": "10.6.2",
        "npm-run-all": "4.1.5",
        "nuxt": "4.1.3",
        "nuxt-viewport": "2.3.1",
        "pinia-plugin-persistedstate": "^4.7.1",
        "prettier-plugin-tailwindcss": "0.7.2",
        "stylelint": "16.26.1",
        "stylelint-config-tailwindcss": "^1.0.0",
        "typescript": "5.9.3",
        "vue": "3.5.25",
        "vue-router": "4.6.4",
        "vue-tsc": "^3.1.8"
    },
    "dependencies": {
        "@effect-digital/vue-utils": "^1.0.35",
        "@marker.io/browser": "0.20.2",
        "@nuxt/image": "^2.0.0",
        "@nuxt/ui": "^4.2.1",
        "@pinia/nuxt": "^0.11.3",
        "@tailwindcss/vite": "^4.1.18",
        "isomorphic-dompurify": "2.26.0",
        "pinia": "^3.0.4",
        "stylelint-config-standard": "^39.0.1",
        "stylelint-prettier": "5.0.3",
        "tailwindcss": "^4.1.18"
    }
}
